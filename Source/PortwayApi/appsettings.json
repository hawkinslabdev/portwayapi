{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Swagger": {
    "Enabled": true,
    "Title": "Portway: API for Data Integration",
    "Version": "v1", 
    "Description": "A lightweight gateway API for data integration",
    "Contact": {
      "Name": "Eddie Munson",
      "Email": "support@hawkinlabindustries.com"
    },
    "SecurityDefinition": {
      "Name": "Bearer",
      "Description": "JWT Authorization header using the Bearer scheme. Example: \"Bearer {token}\"",
      "In": "Header", 
      "Type": "ApiKey",
      "Scheme": "Bearer"
    },
    "RoutePrefix": "swagger",
    "DocExpansion": "List",
    "DefaultModelsExpandDepth": -1,
    "DisplayRequestDuration": true,
    "EnableFilter": false,
    "EnableDeepLinking": false,
    "EnableValidator": false
  },
    "RateLimiting": {
      "Enabled": true,
      "IpLimit": 100,
      "IpWindow": 60,
      "TokenLimit": 1000,
      "TokenWindow": 60
    }
  
}